						<div class="navi"></div> 
					
			</div>
	
	<script type="text/javascript">
	
		$(function() { 
			var totalPages;
			if(true){     
					// Initialize scrollable . Speed is set to 0 so that there wont be scrolling animation when the page is initially loaded.
					// Target the Id instead of generic div.scrollable to allow having multiple carousels with different behaviors in the same page.
					  var api= $('#global-footer-carousel div.scrollable').scrollable({
							 clickable:false,
							 size:3,
							 speed:0,
							 keyboard: false,
							 api:true
							 }); 
					
					totalPages =api.getPageAmount();
					var randomPageIndex=Math.floor(Math.random()*totalPages);
					var firstTime =true;
					api.onSeek(function() { 
						if(firstTime){
							this.getConf().speed = 400; 
							firstTime=false;
						}
					});
						
						api.setPage(randomPageIndex);			
			}else{
				
				  var api2= $('#global-footer-carousel div.scrollable').scrollable({
							 clickable:false,
							 size:3,
							 speed:400,
							 api:true
							 });
					totalPages =api2.getPageAmount();
		    }
			 // If total page is just 1 hide nextPage and navi 
            if(totalPages==1){
                if(true){
                    $('#global-footer-carousel div.navi').hide();
                }
                $('#global-footer-carousel .nextPage').hide();
            }

            // Center navigation on the fly
            if(true){
                var defaultW = 13;
                var singleNavW=Number($('#global-footer-carousel div.navi a').width());
                singleNavW = singleNavW > 0 ? singleNavW : defaultW;
                var marginL=Number($('#global-footer-carousel div.navi a').css("margin-left").replace("px", ""));
                var marginR=Number($('#global-footer-carousel div.navi a').css("margin-right").replace("px", ""));  
                var totalNavW=String(totalPages*(singleNavW +marginL+marginR)+3);
                $("#global-footer-carousel div.navi").width(totalNavW+'px'); 
            }
          
		});	
	</script></div><div class="line copyright center"><div><a target="_self" href="https://web.archive.org/web/20141203022037/http://www.nickjr.com/about/privacy-policy.html" rel="nofollow">Privacy Policy (updated 08/29/2014)</a> | 
