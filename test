			   player = controller.player;
			   player.addEventListener('READY','onReady');  
			   player.addEventListener('PLAYLIST_COMPLETE','onPlaylistComplete');  
			};
			function onReady() {	
				// console.log("Player On Ready");
				forcedMutePlayerCheck("nickjrHomepage");
				
